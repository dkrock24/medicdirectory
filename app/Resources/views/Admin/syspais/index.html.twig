{% extends 'Layout/base_admin/_admin_base.html.twig' %}

{% block body %}
    <h1>Syspais list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombresyspais</th>
                <th>Prefijosyspais</th>
                <th>Codigopostalsyspais</th>
                <th>Continentepaissyspais</th>
                <th>Estatussyspais</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sysPai in sysPais %}
            <tr>
                <td><a href="{{ path('sys_pais_show', { 'id': sysPai.id }) }}">{{ sysPai.id }}</a></td>
                <td>{{ sysPai.nombreSysPais }}</td>
                <td>{{ sysPai.prefijoSysPais }}</td>
                <td>{{ sysPai.codigoPostalSysPais }}</td>
                <td>{{ sysPai.continentePaisSysPais }}</td>
                <td>{% if sysPai.estatusSysPais %}Yes{% else %}No{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('sys_pais_show', { 'id': sysPai.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('sys_pais_edit', { 'id': sysPai.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('sys_pais_new') }}">Create a new sysPai</a>
        </li>
    </ul>
{% endblock %}
