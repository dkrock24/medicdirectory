<script>
    $(document).on("ready",function(){
        
        if( $("#appbundle_menu_menIcono").size() > 0 )
        {
            $("#appbundle_menu_menIcono").closest("div").append("<div id='current_icon' class='text-right'></div>");
            var icon = $("#appbundle_menu_menIcono").val();
            $("#current_icon").html("<i class='"+icon+"'></i>");
        }
        
        $(".fontawesome-icon-list button").on("click",function(){
            removeSelected();
            $(this).addClass("btn-selected");
        });
        
        $("#menu_menIcono").focus(function(){
            removeSelected();
        });
        
        $("#btn_set_icon").on("click",function(){
            if( $(".btn-selected").size() > 0 )
            {
               var icon = $(".btn-selected").find("i").attr("class"); 
               $("#appbundle_menu_menIcono").val(icon);
               $("#current_icon").html("<i class='"+icon+"'></i>");
               $('.modal').modal("hide");
            }
        });
        
        
        
        function removeSelected(){
            $(".btn-selected").removeClass("btn-selected");
        }
        
        setPermission();
        function setPermission(){
            {% if currentPermissions is defined %}
                {% for permission in currentPermissions %}
                       var id = "{{ permission.menRolRol.rolId }}";
                       console.log(id);
                       $("#appbundle_menu_permisos option[value="+id+"]").attr('selected','selected');

                {% endfor %}
            {% endif %}    
        }
    });
</script>
       