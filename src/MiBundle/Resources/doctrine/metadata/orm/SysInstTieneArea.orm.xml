<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysInstTieneArea" table="sys_inst_tiene_area">
    <indexes>
      <index name="fk_sys_inst_tiene_area_sys_area1_idx" columns="sys_area_id"/>
      <index name="fk_sys_inst_tiene_area_sys_institucion1_idx" columns="sys_institucion_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="estatusIta" type="boolean" column="estatus_ita" nullable="true"/>
    <many-to-one field="sysInstitucion" target-entity="SysInstitucion" fetch="LAZY">
      <join-columns>
        <join-column name="sys_institucion_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sysArea" target-entity="SysArea" fetch="LAZY">
      <join-columns>
        <join-column name="sys_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
