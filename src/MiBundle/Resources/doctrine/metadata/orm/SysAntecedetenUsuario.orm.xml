<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysAntecedetenUsuario" table="sys_antecedeten_usuario">
    <indexes>
      <index name="FK_sys_antecedeten_usuario_sys_antecedentes_especialidad" columns="id_antecedente_especialidad"/>
      <index name="FK_sys_antecedeten_usuario_sys_usuario" columns="id_usuario"/>
      <index name="FK_sys_antecedeten_usuario_sys_cita" columns="id_cita"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="integer" column="descripcion" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaCreacion" type="date" column="fecha_creacion" nullable="true"/>
    <many-to-one field="idUsuario" target-entity="SysUsuario" fetch="LAZY">
      <join-columns>
        <join-column name="id_usuario" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAntecedenteEspecialidad" target-entity="SysAntecedentesEspecialidad" fetch="LAZY">
      <join-columns>
        <join-column name="id_antecedente_especialidad" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCita" target-entity="SysCita" fetch="LAZY">
      <join-columns>
        <join-column name="id_cita" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
