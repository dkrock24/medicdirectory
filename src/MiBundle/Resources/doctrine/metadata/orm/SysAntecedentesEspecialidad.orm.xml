<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysAntecedentesEspecialidad" table="sys_antecedentes_especialidad">
    <indexes>
      <index name="FK_sys_antecedentes_especialidad_sys_especialidades" columns="id_especialidad"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="antecedente" type="string" column="antecedente" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="valor" type="string" column="valor" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcion" type="text" column="descripcion" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaCreacion" type="datetime" column="fecha_creacion" nullable="true"/>
    <many-to-one field="idEspecialidad" target-entity="SysEspecialidades" fetch="LAZY">
      <join-columns>
        <join-column name="id_especialidad" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
