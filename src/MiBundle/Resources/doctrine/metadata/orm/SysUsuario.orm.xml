<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysUsuario" table="sys_usuario">
    <indexes>
      <index name="FK_sys_roles" columns="id_rol"/>
      <index name="FK_sys_municipio" columns="id_municipio"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="email" type="string" column="email" length="60" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activo" type="boolean" column="activo" nullable="true"/>
    <field name="password" type="string" column="password" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tarjetaCredito" type="string" column="tarjeta_credito" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usuario" type="string" column="usuario" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaActualizacion" type="datetime" column="fecha_actualizacion" nullable="true"/>
    <field name="genero" type="string" column="genero" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ocupacion" type="string" column="ocupacion" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="responsableDePaciente" type="string" column="responsable_de_paciente" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numeroAfp" type="integer" column="numero_afp" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numeroNup" type="integer" column="numero_nup" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellido" type="string" column="apellido" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaNacimiento" type="date" column="fecha_nacimiento" nullable="true"/>
    <field name="celular" type="string" column="celular" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jvrm" type="string" column="jvrm" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigoPostal" type="integer" column="codigo_postal" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dui" type="string" column="dui" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaCreacion" type="date" column="fecha_creacion" nullable="true"/>
    <many-to-one field="idRol" target-entity="SysRoles" fetch="LAZY">
      <join-columns>
        <join-column name="id_rol" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMunicipio" target-entity="SysMunicipio" fetch="LAZY">
      <join-columns>
        <join-column name="id_municipio" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
