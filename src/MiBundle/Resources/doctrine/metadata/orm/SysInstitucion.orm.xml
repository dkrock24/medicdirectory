<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysInstitucion" table="sys_institucion">
    <indexes>
      <index name="fk_sys_institucion_sys_tipo_institucion_idx" columns="sys_tipo_institucion_id"/>
      <index name="fk_sys_institucion_sys_usuario1_idx" columns="sys_usuario_id"/>
      <index name="fk_sys_institucion_sys_municipio1_idx" columns="sys_municipio_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreSysInstitucion" type="string" column="nombre_sys_institucion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccionSysInstitucion" type="string" column="direccion_sys_institucion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="latitudSysInstitucion" type="string" column="latitud_sys_institucion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="longitudSysInstitucion" type="string" column="longitud_sys_institucion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prefijoSysInstitucion" type="string" column="prefijo_sys_institucion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono1SysInstitucion" type="string" column="telefono1_sys_institucion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono2SysInstitucion" type="string" column="telefono2_sys_institucion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="sysUsuario" target-entity="SysUsuario" fetch="LAZY">
      <join-columns>
        <join-column name="sys_usuario_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sysMunicipio" target-entity="SysMunicipio" fetch="LAZY">
      <join-columns>
        <join-column name="sys_municipio_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sysTipoInstitucion" target-entity="SysTipoInstitucion" fetch="LAZY">
      <join-columns>
        <join-column name="sys_tipo_institucion_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
