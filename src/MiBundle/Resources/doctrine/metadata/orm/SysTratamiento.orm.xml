<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysTratamiento" table="sys_tratamiento">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="via" type="string" column="via" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="docis" type="string" column="docis" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="preriocidad" type="string" column="preriocidad" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="idCita" type="integer" column="id_cita" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tipoTratamiento" type="string" column="tipo_tratamiento" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
