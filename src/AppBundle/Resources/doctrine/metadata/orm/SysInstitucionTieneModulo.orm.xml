<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysInstitucionTieneModulo" table="sys_institucion_tiene_modulo">
    <indexes>
      <index name="FK_sys_institucion_tiene_modulo_sys_modulos" columns="sys_modulo_id"/>
      <index name="FK_sys_institucion_tiene_modulo_sys_institucion" columns="sys_institucion_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="estatusSysUtm" type="boolean" column="estatus_sys_utm" nullable="true"/>
    <field name="fechaInicioSysUtm" type="date" column="fecha_inicio_sys_utm" nullable="false"/>
    <field name="fechaFinSysUtm" type="date" column="fecha_fin_sys_utm" nullable="false"/>
    <field name="totalCostoEspecialSysUtm" type="float" column="total_costo_especial_sys_utm" precision="10" scale="0" nullable="false"/>
    <field name="totalMesesSysUsuarioTieneModulo" type="string" column="total_meses_sys_usuario_tiene_modulo" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="sysInstitucion" target-entity="SysInstitucion" fetch="LAZY">
      <join-columns>
        <join-column name="sys_institucion_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sysModulo" target-entity="SysModulos" fetch="LAZY">
      <join-columns>
        <join-column name="sys_modulo_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
