<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysUsuarioTieneRol" table="sys_usuario_tiene_rol">
    <indexes>
      <index name="fk_sys_usuario_tiene_rol_sys_usuario1_idx" columns="sys_usuario_id"/>
      <index name="fk_sys_usuario_tiene_rol_sys_roles1_idx" columns="sys_roles_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="estadoSysUsuarioTieneRol" type="string" column="estado_sys_usuario_tiene_rol" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="sysUsuario" target-entity="SysUsuario" fetch="LAZY">
      <join-columns>
        <join-column name="sys_usuario_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sysRoles" target-entity="SysRoles" fetch="LAZY">
      <join-columns>
        <join-column name="sys_roles_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
