<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysPreferencias" table="sys_preferencias">
    <indexes>
      <index name="FK_sys_preferencias_sys_usuario" columns="sys_usuario_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fechaCreacion" type="datetime" column="fecha_creacion" nullable="true"/>
    <many-to-one field="sysUsuario" target-entity="SysUsuario" fetch="LAZY">
      <join-columns>
        <join-column name="sys_usuario_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
