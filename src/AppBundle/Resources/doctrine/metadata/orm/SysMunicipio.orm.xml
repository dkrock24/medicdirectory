<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysMunicipio" table="sys_municipio">
    <indexes>
      <index name="fk_sys_minicipio_sys_departamentos1_idx" columns="sys_departamentos_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreSysMunicipo" type="string" column="nombre_sys_municipo" length="60" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estatusSysMunicipo" type="boolean" column="estatus_sys_municipo" nullable="true"/>
    <field name="descripcionSysMunicipio" type="string" column="descripcion_sys_municipio" length="60" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="sysDepartamentos" target-entity="SysDepartamentos" fetch="LAZY">
      <join-columns>
        <join-column name="sys_departamentos_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
