<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysSugerencias" table="sys_sugerencias">
    <indexes>
      <index name="fk_sys_sugerencias_sys_institucion1_idx" columns="sys_institucion_id"/>
      <index name="fk_sys_sugerencias_sys_area1_idx" columns="sys_area_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="comentarioSysSugerencia" type="text" column="comentario_sys_sugerencia" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaCreacionSysSugerencia" type="datetime" column="fecha_creacion_sys_sugerencia" nullable="true"/>
    <field name="estatusSugerencia" type="boolean" column="estatus_sugerencia" nullable="true"/>
    <many-to-one field="sysInstitucion" target-entity="SysInstitucion" fetch="LAZY">
      <join-columns>
        <join-column name="sys_institucion_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sysArea" target-entity="SysArea" fetch="LAZY">
      <join-columns>
        <join-column name="sys_area_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
