<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysExamenesMedicos" table="sys_examenes_medicos">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreExamen" type="string" column="nombre_examen" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipoExamen" type="string" column="tipo_examen" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoExamen" type="boolean" column="estado_examen" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaCreacion" type="datetime" column="fecha_creacion" nullable="true"/>
  </entity>
</doctrine-mapping>
