<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysUsuarioTieneEspecialidad" table="sys_usuario_tiene_especialidad">
    <indexes>
      <index name="fk_sys_usuario_tiene_especialidad_sys_especialidades1_idx" columns="sys_especialidades_id"/>
      <index name="fk_sys_usuario_tiene_especialidad_sys_usuario1_idx" columns="sys_usuario_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="subCategoriaSysUte" type="string" column="sub_categoria_sys_ute" length="60" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcionSysUte" type="text" column="descripcion_sys_ute" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estatusSysUte" type="boolean" column="estatus_sys_ute" nullable="true"/>
    <many-to-one field="sysUsuario" target-entity="SysUsuario" fetch="LAZY">
      <join-columns>
        <join-column name="sys_usuario_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sysEspecialidades" target-entity="SysEspecialidades" fetch="LAZY">
      <join-columns>
        <join-column name="sys_especialidades_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
