<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysExamenesMedicosTipos" table="sys_examenes_medicos_tipos">
    <indexes>
      <index name="FK_sys_examenes_medicos_tipos_sys_usuario" columns="id_medico"/>
      <index name="FK_sys_examenes_medicos_tipos_sys_examenes_medicos" columns="id_examen_medico"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="estado" type="integer" column="estado" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fechaCreacion" type="datetime" column="fecha_creacion" nullable="true"/>
    <many-to-one field="idExamenMedico" target-entity="SysExamenesMedicos" fetch="LAZY">
      <join-columns>
        <join-column name="id_examen_medico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMedico" target-entity="SysUsuario" fetch="LAZY">
      <join-columns>
        <join-column name="id_medico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
