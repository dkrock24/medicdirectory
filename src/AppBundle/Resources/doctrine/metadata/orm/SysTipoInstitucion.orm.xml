<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysTipoInstitucion" table="sys_tipo_institucion">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreTipoSysInstitucion" type="string" column="nombre_tipo_sys_institucion" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcionSysTipoInstitucion" type="text" column="descripcion_sys_tipo_institucion" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estadoSysTipoInstitucion" type="integer" column="estado_sys_tipo_institucion" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="codigoSysTipoInstitucion" type="string" column="codigo_sys_tipo_institucion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaCrecionSysTipoInstitucion" type="date" column="fecha_crecion_sys_tipo_institucion" nullable="true"/>
  </entity>
</doctrine-mapping>
