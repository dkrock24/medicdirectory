<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SysPais" table="sys_pais">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreSysPais" type="string" column="nombre_sys_pais" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prefijoSysPais" type="string" column="prefijo_sys_pais" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigoPostalSysPais" type="integer" column="codigo_postal_sys_pais" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="continentePaisSysPais" type="string" column="continente_pais_sys_pais" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estatusSysPais" type="boolean" column="estatus_sys_pais" nullable="true"/>
  </entity>
</doctrine-mapping>
