{% extends 'base.html.twig' %}

{% block breadcrumb %}
    {{ include('AppBundle:sysantecedentesespecialidad:_breadcrumb.html.twig' )  }}
{% endblock %} 

{% block submenutop %}
    {{ include('AppBundle:sysantecedentesespecialidad:_submenu.html.twig' )  }}
{% endblock %} 

{% block body %}
    <h1>Antecedente Especialidad</h1>
    {% for flashMessage in app.session.flashbag.get('success') %}
        <div class="alert alert-dismissible alert-success"><span class="glyphicon glyphicon-ok-sign"></span> {{ flashMessage }}</div>
    {% endfor %}
    {% for flashMessage in app.session.flashbag.get('error') %}
        <div class="alert alert-dismissible alert-danger"><span class="glyphicon glyphicon-exclamation-sign"></span> {{ flashMessage }}</div>
    {% endfor %}

<div class="row">
    <div class="col-lg-6 col-md-6 col-sm-12">
    <table class="table table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sysAntecedentesEspecialidad.id }}</td>
            </tr>
            <tr>
                <th>Antecedente</th>
                <td>{{ sysAntecedentesEspecialidad.antecedente }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ sysAntecedentesEspecialidad.valor }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ sysAntecedentesEspecialidad.descripcion }}</td>
            </tr>
            <tr>
                <th>Especialidad</th>
                <td>{{ sysAntecedentesEspecialidad.idEspecialidad }}</td>
            </tr>
            <tr>
                <th>Fechacreacion</th>
                <td>{% if sysAntecedentesEspecialidad.fechaCreacion %}{{ sysAntecedentesEspecialidad.fechaCreacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>
    <hr>
    </div>
</div>

    <ul class="btn-option-emr">
        <li>
            <a href="{{ path('sysantecedentesespecialidad_index') }}" class="btn btn-default"><i class="glyphicon glyphicon-th-list"></i> Regresar a la lista</a>
        </li>
        <li>
            <a href="{{ path('sysantecedentesespecialidad_edit', { 'id': sysAntecedentesEspecialidad.id }) }}" class="btn btn-info"><i class="glyphicon glyphicon-pencil"></i> Editar</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Eliminar" class="btn btn-danger">
            {{ form_end(delete_form) }}
        </li>
        <li>
            <a href="{{ path('sysantecedentesespecialidad_new')}}" class="btn btn-inverse"><i class="fa fa-plus"></i> Nuevo </a>
        </li>
    </ul>
{% endblock %}
