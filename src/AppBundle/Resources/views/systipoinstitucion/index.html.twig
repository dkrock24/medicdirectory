{% extends 'base.html.twig' %}

{% block body %}
    <h1>SysTipoInstitucion list</h1>

    <table>
        <thead>
            <tr>
                <th>Idsysinstitucion</th>
                <th>Nombretiposysinstitucion</th>
                <th>Descripcionsystipoinstitucion</th>
                <th>Estadosystipoinstitucion</th>
                <th>Codigosystipoinstitucion</th>
                <th>Fechacrecionsystipoinstitucion</th>
                <th>Idmunicipio</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sysTipoInstitucion in sysTipoInstitucions %}
            <tr>
                <td><a href="{{ path('systipoinstitucion_show', { 'id': sysTipoInstitucion.idSysInstitucion }) }}">{{ sysTipoInstitucion.idSysInstitucion }}</a></td>
                <td>{{ sysTipoInstitucion.nombreTipoSysInstitucion }}</td>
                <td>{{ sysTipoInstitucion.descripcionSysTipoInstitucion }}</td>
                <td>{{ sysTipoInstitucion.estadoSysTipoInstitucion }}</td>
                <td>{{ sysTipoInstitucion.codigoSysTipoInstitucion }}</td>
                <td>{% if sysTipoInstitucion.fechaCrecionSysTipoInstitucion %}{{ sysTipoInstitucion.fechaCrecionSysTipoInstitucion|date('Y-m-d') }}{% endif %}</td>
                <td>{{ sysTipoInstitucion.idMunicipio }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('systipoinstitucion_show', { 'id': sysTipoInstitucion.idSysInstitucion }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('systipoinstitucion_edit', { 'id': sysTipoInstitucion.idSysInstitucion }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('systipoinstitucion_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
