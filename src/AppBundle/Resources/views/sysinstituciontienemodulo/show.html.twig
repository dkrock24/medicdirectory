{% extends '_admin_base.html.twig' %}

{% block breadcrumb %}
    {{ include('AppBundle:sysInstitucionTieneModulo:_breadcrumb.html.twig' )  }}
{% endblock %} 

{% block submenutop %}
    {{ include('AppBundle:sysInstitucionTieneModulo:_submenu.html.twig' )  }}
{% endblock %} 

{% block body %}
    <h1>Modulos Institucion</h1>
    {% for flashMessage in app.session.flashbag.get('success') %}
        <div class="alert alert-dismissible alert-success"><span class="glyphicon glyphicon-ok-sign"></span> {{ flashMessage }}</div>
    {% endfor %}
    {% for flashMessage in app.session.flashbag.get('error') %}
        <div class="alert alert-dismissible alert-danger"><span class="glyphicon glyphicon-exclamation-sign"></span> {{ flashMessage }}</div>
    {% endfor %}

<div class="row">
    <div class="col-lg-6 col-md-6 col-sm-12">
    <table class="table table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sysInstitucionTieneModulo.id }}</td>
            </tr>
            
            <tr>
                <th>Fecha Inicio</th>
                <td>{% if sysInstitucionTieneModulo.fechaInicioSysUtm %}{{ sysInstitucionTieneModulo.fechaInicioSysUtm|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fecha Fin</th>
                <td>{% if sysInstitucionTieneModulo.fechaFinSysUtm %}{{ sysInstitucionTieneModulo.fechaFinSysUtm|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Total Costo</th>
                <td>{{ sysInstitucionTieneModulo.totalCostoEspecialSysUtm }}</td>
            </tr>
            <tr>
                <th>Total Meses</th>
                <td>{{ sysInstitucionTieneModulo.totalMesesSysUsuarioTieneModulo }}</td>
            </tr>
            <tr>
                <th>Institucion</th>
                <td>{{ sysInstitucionTieneModulo.sysInstitucion }}</td>
            </tr>
            <tr>
                <th>Modulo</th>
                <td>{{ sysInstitucionTieneModulo.sysModulo }}</td>
            </tr>
            <tr>
                <th>Estatus</th>
                <td>{% if sysInstitucionTieneModulo.estatusSysUtm %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>
 <hr>
</div>
</div>
<ul class="btn-option-emr">
        <li>
            <a href="{{ path('sysinstituciontienemodulo_index') }}" class="btn btn-default"><i class="glyphicon glyphicon-th-list"></i> Regresar a la lista</a>
        </li>
        <li>
            <a href="{{ path('sysinstituciontienemodulo_edit', { 'id': sysInstitucionTieneModulo.id }) }}" class="btn btn-info"><i class="glyphicon glyphicon-pencil"></i> Editar</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                 <input type="submit" value="Eliminar" class="btn btn-danger">
            {{ form_end(delete_form) }}
        </li>
        <li>
            <a href="{{ path('sysinstituciontienemodulo_new')}}" class="btn btn-inverse"><i class="fa fa-plus"></i> Nuevo </a>
        </li>
    </ul>
{% endblock %}
