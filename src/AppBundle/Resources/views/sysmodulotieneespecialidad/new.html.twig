{% extends 'base.html.twig' %}

{% block breadcrumb %}
    {{ include('AppBundle:sysmodulotieneespecialidad:_breadcrumb.html.twig' )  }}
{% endblock %} 

{% block submenutop %}
    {{ include('AppBundle:sysmodulotieneespecialidad:_submenu.html.twig' )  }}
{% endblock %} 

{% block body %}
   <h1>Nueva entrada</h1>

   <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-12">
            <div class="panel-bodyx">
                {{ form_start(form) }}
                    {# {{ dump( form_widget(form) )}} #}

                    <div class="form-group">
                        <div class="form_error">{{ form_errors(form.sysModulos)}}</div>
                        <div><label for="">Modulo</label></div>
                        {{ form_widget(form.sysModulos) }}
                    </div>

                    <div class="form-group">
                        <div class="form_error">{{ form_errors(form.sysEspecialidades)}}</div>
                        <div><label for="">Especialidad</label></div>
                        {{ form_widget(form.sysEspecialidades) }}
                    </div>

                    <div class="form-group">
                        <div class="form_error">{{ form_errors(form.sysEstatus)}}</div>
                        <div>Estatus</div>
                        <div class="checkbox">
                            <label>
                                {{ form_widget(form.sysEstatus) }}
                                <span class="text"></span>
                            </label>
                        </div>   
                    </div>



                    <input type="submit" value="Crear" class="btn btn-primary" />
                {{ form_end(form) }}
            </div>
        </div>  
    </div>
{% endblock %}
