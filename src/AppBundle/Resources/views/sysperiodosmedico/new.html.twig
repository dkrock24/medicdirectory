{% extends 'base.html.twig' %}

{% block breadcrumb %}
    {{ include('AppBundle:sysperiodosmedico:_breadcrumb.html.twig' )  }}
{% endblock %} 

{% block submenutop %}
    {{ include('AppBundle:sysperiodosmedico:_submenu.html.twig' )  }}
{% endblock %} 

{% block body %}
    <h1>Nueva entrada</h1>

    <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-12">

            {{ form_start(form) }}
                <div class="form_error">{{ form_errors(form)}}</div>

                <div class="form-group">                    
                    <div class="form_error">{{ form_errors(form.fechaContratoInico)}}</div>
                    <div><label for="">Fecha Inicio</label></div>
                    {{ form_widget(form.fechaContratoInico) }}
                </div>

                <div class="form-group">                    
                    <div class="form_error">{{ form_errors(form.fechaContratoFin)}}</div>
                    <div><label for="">Fecha Fin</label></div>
                    {{ form_widget(form.fechaContratoFin) }}
                </div>

                <div class="form-group">                    
                    <div class="form_error">{{ form_errors(form.montoSysPeriodosMedico)}}</div>
                    <div><label for="">Monto Por Periodo</label></div>
                    {{ form_widget(form.montoSysPeriodosMedico) }}
                </div>

                <div class="form-group">                    
                    <div class="form_error">{{ form_errors(form.sysUsuario)}}</div>
                    <div><label for="">Usuario</label></div>
                    {{ form_widget(form.sysUsuario) }}
                </div>
               
                 <input type="submit" value="Crear" class="btn btn-primary" />
            {{ form_end(form) }}

        </div>
    </div>

{% endblock %}
