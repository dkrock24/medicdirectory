{% extends "::base.html.twig" %}
  
{% block body %}
    <div class="row">
        <div class="col-lg-12">
            <h1>SysMunicipio</h1>

            <table>
                <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ sysMunicipio.id }}</td>
                    </tr>
                    <tr>
                        <th>Nombresysmunicipo</th>
                        <td>{{ sysMunicipio.nombreSysMunicipo }}</td>
                    </tr>
                    <tr>
                        <th>Esactivosysmunicipo</th>
                        <td>{% if sysMunicipio.esActivoSysMunicipo %}Yes{% else %}No{% endif %}</td>
                    </tr>
                    <tr>
                        <th>Descripcionsysmunicipio</th>
                        <td>{{ sysMunicipio.descripcionSysMunicipio }}</td>
                    </tr>
                </tbody>
            </table>

            <ul>
                <li>
                    <a href="{{ path('sysmunicipio_index') }}">Back to the list</a>
                </li>
                <li>
                    <a href="{{ path('sysmunicipio_edit', { 'id': sysMunicipio.id }) }}">Edit</a>
                </li>
                <li>
                    {{ form_start(delete_form) }}
                    <input type="submit" value="Delete">
                    {{ form_end(delete_form) }}
                </li>
            </ul>

        </div>
    </div>
{% endblock %}

{% block codejs %}
    <script>
        $(function(){

          });
    </script>
{% endblock %} 