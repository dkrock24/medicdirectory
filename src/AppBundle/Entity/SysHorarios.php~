<?php

namespace AppBundle\Entity;

/**
 * SysHorarios
 */
class SysHorarios
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var \DateTime
     */
    private $diaSysHorario;

    /**
     * @var string
     */
    private $entradaSysHorario;

    /**
     * @var string
     */
    private $salidaSysHorario;

    /**
     * @var \AppBundle\Entity\SysUsuario
     */
    private $sysUsuario;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set diaSysHorario
     *
     * @param \DateTime $diaSysHorario
     *
     * @return SysHorarios
     */
    public function setDiaSysHorario($diaSysHorario)
    {
        $this->diaSysHorario = $diaSysHorario;

        return $this;
    }

    /**
     * Get diaSysHorario
     *
     * @return \DateTime
     */
    public function getDiaSysHorario()
    {
        return $this->diaSysHorario;
    }

    /**
     * Set entradaSysHorario
     *
     * @param string $entradaSysHorario
     *
     * @return SysHorarios
     */
    public function setEntradaSysHorario($entradaSysHorario)
    {
        $this->entradaSysHorario = $entradaSysHorario;

        return $this;
    }

    /**
     * Get entradaSysHorario
     *
     * @return string
     */
    public function getEntradaSysHorario()
    {
        return $this->entradaSysHorario;
    }

    /**
     * Set salidaSysHorario
     *
     * @param string $salidaSysHorario
     *
     * @return SysHorarios
     */
    public function setSalidaSysHorario($salidaSysHorario)
    {
        $this->salidaSysHorario = $salidaSysHorario;

        return $this;
    }

    /**
     * Get salidaSysHorario
     *
     * @return string
     */
    public function getSalidaSysHorario()
    {
        return $this->salidaSysHorario;
    }

    /**
     * Set sysUsuario
     *
     * @param \AppBundle\Entity\SysUsuario $sysUsuario
     *
     * @return SysHorarios
     */
    public function setSysUsuario(\AppBundle\Entity\SysUsuario $sysUsuario = null)
    {
        $this->sysUsuario = $sysUsuario;

        return $this;
    }

    /**
     * Get sysUsuario
     *
     * @return \AppBundle\Entity\SysUsuario
     */
    public function getSysUsuario()
    {
        return $this->sysUsuario;
    }
}

